#include <iostream>
using namespace std;
int main()
{
    int A[11],n=11,key;
    cout<<"Enter no. ";
    for(int i=0;i<11;++i)
    {
        cin>>A[i];
    }
    cout<<"Enter key "<<endl;
    cin>>key;
    for(int i=0;i<11;++i)
    {
        if(key==A[i])
        {
            cout<<"found at "<<i;
            return 0;
        }
    }
    cout<<"not found";
    return 0;
}
