#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int age;
    cout<<"Enter your age";
    cin>>age;
    if(age<12 || age>50)
    {
        cout<<"Congrats you are eligible for the price";
    }
    else 
    {
        cout<<"Sorry you are not eligible";
    }
    return 0;
}
