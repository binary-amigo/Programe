#include <iostream>
using namespace std;
int main()
{
    string str,rev;
    cout<<"Enter string: ";
    cin>>str;
    int len=(int)str.length();
    rev.resize(len);
    for(int i=0,j=(len-1);i<len;i++,j--)
    {
        rev[i]=str[j];
    }
    rev[len]='\0';
    if(rev.compare(str)==0)
    cout<<"Palindrome";
    else
    cout<<"Not palindrome";
    
}
