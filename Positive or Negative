#include <iostream>
using namespace std;
int main()
{
    int x;
    cout<<"Enter your no.";
    cin>>x;
    if(x>0)
    {
        cout<<x<<" is positive";
    }
    else if (x==0)
    {
        cout<<x<<" neither positive nor negative";
    }
    else
    {
        cout<<x<<" is negative";
    }
    
    return 0;
}
