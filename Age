#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int age;
    cout<<"Enter your age";
    cin>>age;
    if(age>=12 && age<=50)
    {
        cout<<"You'r young pal";
    }
    else if(age<=12)
    {
        cout<<"Hello kiddo";
    }
    else
    {
        cout<<"Hey! Grandpa";
    }
    return 0;
}
