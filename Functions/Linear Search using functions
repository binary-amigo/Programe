#include <iostream>
using namespace std;
int linear_search(int arr[],int B,int C)
{
    for(int i=0;i<B;++i)
    {
        if(arr[i]==C)
        return i;
    }
    return -1;
}
int main()
{
    int arr[10]={1,3,56,34,32,55,89,30,70,100};
    int key;
    cin>>key;
    int ans=linear_search(arr,10,key);
    cout<<ans;
}
