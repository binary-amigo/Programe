#include <iostream>
using namespace std;
int main()
{
    int n,i,sum=0;
    cout<<"Enter n ";
    cin>>n;
    for(i=1;i<=n;++i)
    {
        if(n%i==0)
        {
            sum+=i;
        }
    }
    if(sum==2*n)
    {
        cout<<n<<" is a prime no.";
    }
    else
    {
        cout<<n<<" is not a prime no.";
    }
    return 0;
}
